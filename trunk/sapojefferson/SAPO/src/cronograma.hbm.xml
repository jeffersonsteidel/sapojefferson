<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
	PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="br.com.siscone.entity.Cronograma" table="cronograma">
		<id name="codigo" column="CRO_COD">
			<generator class="increment"></generator>
		</id>
		<property name="dataIniCadastrar" column="CRO_DT_INI_CAD" />
		<property name="dataFimCadastrar" column="CRO_DT_FIM_CAD" />
		<property name="dataIniPriorizar" column="CRO_DT_INI_PRI" />
		<property name="dataFimPriorizar" column="CRO_DT_FIM_PRI" />
		<property name="dataIniAprovar" column="CRO_DT_INI_APR" />
		<property name="dataFimAprovar" column="CRO_DT_FIM_APR" />
		<property name="dataIniReplanejamento" column="CRO_DT_INI_REPLAN" />
		<property name="dataFimReplanejamento" column="CRO_DT_FIM_REPLAN" />
	</class>
</hibernate-mapping>