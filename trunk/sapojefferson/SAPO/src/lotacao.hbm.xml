<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
	PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="br.com.siscone.entity.Lotacao" table="lotacao">
		<id name="codigo" column="LOT_COD">
			<generator class="increment"></generator>
		</id>
		<property name="descricao" column="LOT_DESC" />
		<property name="orcamentoCapital" column="LOT_ORC_CAPITAL" />
		<property name="orcamentoCusteio" column="LOT_ORC_CUSTEIO" />
	</class>
</hibernate-mapping>