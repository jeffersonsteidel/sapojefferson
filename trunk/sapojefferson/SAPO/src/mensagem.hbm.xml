<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
	PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="br.com.siscone.entity.Mensagem" table="mensagem">
		<id name="codigo" column="MEN_COD">
			<generator class="increment"></generator>
		</id>
		<property name="descricao" column="MEN_DESC" />
		<property name="data" column="MEN_DATE" />
		<property name="titulo" column="MEN_TITULO" />
	</class>
</hibernate-mapping>